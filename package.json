{
  "name": "web-admin",
  "private": true,
  "description": "eero's internal admin panel",
  "author": "eero Inc (http://github.com/eero-inc)",
  "license": "LicenseRef-LICENSE",
  "version": "2017.3.29-2",
  "repository": {
    "type": "git",
    "url": "https://github.com/eero-inc/web-admin"
  },
  "homepage": "https://admin.e2ro.com",
  "main": "bin/server.js",
  "scripts": {
    "start": "npm run start-prod",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently -k \"npm run watch-client | bunyan\" \"npm run start-dev | bunyan\"",
    "test": "concurrently \"npm run lint\" \"karma start\""
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
